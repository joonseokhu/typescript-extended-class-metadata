{
  "name": "typescript-plugin-extended-class-metadata",
  "version": "0.0.1",
  "description": "",
  "author": "Joonseok Hu",
  "repository": {
    "type": "git",
    "url": "https://github.com/joonseokhu/typescript-plugin-extended-class-metadata.git"
  },
  "keywords": [],
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/lib/plugin.d.ts",
        "default": "./dist/lib/plugin.js"
      },
      "require": {
        "types": "./dist/plugin/index.d.ts",
        "default": "./dist/plugin/index.js"
      }
    }
  },
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist test/dist",
    "build": "npm run prebuild && tsc --project ./tsconfig.build.json",
    "dev": "npm run prebuild && tsc-watch --onSuccess \"node -r tsconfig-paths/register dist\"",
    "test": "npm run build && tspc --project ./tsconfig.test.json && node ./test/dist",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@nestjs/swagger": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "ts-patch": "^3.0.2",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.x",
    "typescript": ">= 4.9.0"
  }
}
